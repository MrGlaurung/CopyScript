      <include name="src/test/java"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.filemover" depends="compile.module.filemover.production,compile.module.filemover.tests" description="Compile module FileMover"/>
  
  <target name="compile.module.filemover.production" depends="register.custom.compilers" description="Compile module FileMover; production classes">
    <mkdir dir="${filemover.output.dir}"/>
    <javac2 destdir="${filemover.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.filemover}/javac">
      <compilerarg line="${compiler.args.filemover}"/>
      <bootclasspath refid="filemover.module.bootclasspath"/>
      <classpath refid="filemover.module.production.classpath"/>
      <src refid="filemover.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.filemover"/>
    </javac2>
    
    <copy todir="${filemover.output.dir}">
      <fileset dir="${module.filemover.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.filemover.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.filemover.tests" depends="register.custom.compilers,compile.module.filemover.production" description="compile module FileMover; test classes" unless="skip.tests">
    <mkdir dir="${filemover.testoutput.dir}"/>
    <javac2 destdir="${filemover.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.filemover}/javac">
      <compilerarg line="${compiler.args.filemover}"/>
      <bootclasspath refid="filemover.module.bootclasspath"/>
      <classpath refid="filemover.module.classpath"/>
      <src refid="filemover.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.filemover"/>
    </javac2>
    
    <copy todir="${filemover.testoutput.dir}">
      <fileset dir="${module.filemover.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.filemover" description="cleanup module">
    <delete dir="${filemover.output.dir}"/>
    <delete dir="${filemover.testoutput.dir}"/>
  </target>
  
